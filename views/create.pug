doctype
html 
    head 
        title Create CV 
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css",rel="stylesheet",integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi",crossorigin="anonymous")
    body 
        div(class="container")
            br
            h1 Create Your CV
            br
            form(action="/submit",method="GET")
                div(class="row g-3 mb-3")
                    div(class="col")
                        label(for="fname",class="form-label") First Name
                    div(class="col")
                        label(for="lname",class="form-label") Last Name
                div(class="row g-3 mb-3")
                    div(class="col")
                        input(type="text",class="form-control", id="fname", name="fname")
                    div(class="col")
                        input(type="text",class="form-control", id="lname", name="lname")
                div(class="mb-3")
                    label(for="email",class="form-label") Email
                    input(type="email",class="form-control", id="email", name="email")
                div(class="mb-3")
                    label(for="profile",class="form-label") Profile
                    textarea(class="form-control", id="profile" rows="3", name="profile")
                div(class="mb-3")
                    label(for="objective",class="form-label") Objective
                    textarea(class="form-control", id="objective" rows="3", name="objective")
                div(class="mb-3")
                    fieldset 
                        legend Education
                        div(class="row g-3 mb-3")
                            div(class="col")
                                label(class="form-label") Type
                            div(class="col")
                                label(class="form-label") Degree
                            div(class="col")
                                label(class="form-label") Grade
                        div(class="row g-3 mb-3")
                            div(class="col")
                                select(class="form-select",name="type[]")
                                    option(value="School") School
                                    option(value="College") College
                                    option(value="University") University
                            div(class="col")
                                input(type="text",class="form-control", name="degree[]")
                            div(class="col")
                                input(type="text",class="form-control", name="grade[]")
                        div(class="row g-3 mb-3")
                            div(class="col")
                                select(class="form-select",name="type[]")
                                    option(value="School") School
                                    option(value="College") College
                                    option(value="University") University
                            div(class="col")
                                input(type="text",class="form-control", name="degree[]")
                            div(class="col")
                                input(type="text",class="form-control", name="grade[]")
                        div(class="row g-3 mb-3")
                            div(class="col")
                                select(class="form-select",name="type[]")
                                    option(value="School") School
                                    option(value="College") College
                                    option(value="University") University
                            div(class="col")
                                input(type="text",class="form-control", name="degree[]")
                            div(class="col")
                                input(type="text",class="form-control", name="grade[]")
                button(type="submit", class="btn btn-primary") Submit
        br
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js",integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3",crossorigin="anonymous")